{
  "name": "@lufa/workspace",
  "private": true,
  "scripts": {
    "build:lib": "pnpm --filter @lufa/design-system run build",
    "dev:apps:storybook": "pnpm --filter @lufa/storybook run dev",
    "--POCs--": "",
    "dev:poc:single-spa-vite-esm": "concurrently \"pnpm --filter @lufa/poc_single-spa-vite-esm_parcel-a run dev\" \"pnpm --filter @lufa/poc_single-spa-vite-esm_main-container dev\"",
    "preview:poc:single-spa-vite-esm": "concurrently \"pnpm --filter @lufa/poc_single-spa-vite-esm_parcel-a run buildAndPreview\" \"pnpm --filter @lufa/poc_single-spa-vite-esm_main-container buildAndPreview\"",
    "--CI/CD--": "",
    "generate-outdated-report": "node .github/scripts/generateOutdatedReport.js"
  },
  "packageManager": "pnpm@10.6.2",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
