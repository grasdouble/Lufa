{
  "name": "@grasdouble/lufa",
  "private": true,
  "scripts": {
    "all-----------------------------------------------": "",
    "all:build": "pnpm --filter @grasdouble/* run build",
    "all:lint": "pnpm --filter @grasdouble/* run lint",
    "all:prettier": "pnpm --filter @grasdouble/* run prettier",
    "app:mf--------------------------------------------": "",
    "app:mf:build": "pnpm --filter @grasdouble/lufa_microfrontend_main-container run build && pnpm --filter @grasdouble/lufa_microfrontend_home run build",
    "app:mf:dev": "concurrently \"pnpm --filter @grasdouble/lufa_microfrontend_main-container run dev\" \"pnpm --filter @grasdouble/lufa_microfrontend_home run dev\"",
    "app:mf:preview": "pnpm --filter @grasdouble/lufa_microfrontend_main-container run preview & pnpm --filter @grasdouble/lufa_microfrontend_home run preview",
    "cdn:autobuild-server-------------------------------": "",
    "cdn:autobuild-server:build": "pnpm --filter @grasdouble/cdn_autobuild-server run build && pnpm --filter @grasdouble/cdn_autobuild-server run build:cjs",
    "cdn:autobuild-server:dev": "pnpm --filter @grasdouble/cdn_autobuild-server run dev",
    "cdn:autobuild-server:lint": "pnpm --filter @grasdouble/cdn_autobuild-server run lint",
    "cdn:autobuild-server:prettier": "pnpm --filter @grasdouble/cdn_autobuild-server run prettier",
    "cdn:autobuild-server:preview": "pnpm --filter @grasdouble/cdn_autobuild-server run preview",
    "ds:all--------------------------------------------": "",
    "ds:all:build": "pnpm ds:tokens:build && pnpm ds:primitives:build && pnpm ds:main:build && pnpm ds:storybook:build && pnpm ds:documentation:build",
    "ds:all:dev": "concurrently \"pnpm ds:main:dev\" \"pnpm ds:storybook:dev\" \"pnpm ds:documentation:dev\"",
    "ds:all:lint": "pnpm ds:tokens:lint && pnpm ds:primitives:lint && pnpm ds:main:lint && pnpm ds:storybook:lint && pnpm ds:documentation:lint",
    "ds:all:prettier": "pnpm ds:tokens:prettier && pnpm ds:primitives:prettier && pnpm ds:main:prettier && pnpm ds:storybook:prettier && pnpm ds:documentation:prettier",
    "ds:documentation----------------------------------": "",
    "ds:documentation:build": "pnpm --filter @grasdouble/lufa_design-system-documentation run build",
    "ds:documentation:dev": "pnpm --filter @grasdouble/lufa_design-system-documentation run dev",
    "ds:documentation:lint": "pnpm --filter @grasdouble/lufa_design-system-documentation run lint",
    "ds:documentation:prettier": "pnpm --filter @grasdouble/lufa_design-system-documentation run prettier",
    "ds:main-------------------------------------------": "",
    "ds:main:build": "pnpm --filter @grasdouble/lufa_design-system run build",
    "ds:main:dev": "pnpm --filter @grasdouble/lufa_design-system run build -w",
    "ds:main:lint": "pnpm --filter @grasdouble/lufa_design-system run lint",
    "ds:main:prettier": "pnpm --filter @grasdouble/lufa_design-system run prettier",
    "ds:primitives-------------------------------------": "",
    "ds:primitives:build": "pnpm --filter @grasdouble/lufa_design-system-primitives run build",
    "ds:primitives:lint": "pnpm --filter @grasdouble/lufa_design-system-primitives run lint",
    "ds:primitives:prettier": "pnpm --filter @grasdouble/lufa_design-system-primitives run prettier",
    "ds:storybook--------------------------------------": "",
    "ds:storybook:build": "pnpm --filter @grasdouble/lufa_design-system-storybook run build",
    "ds:storybook:dev": "pnpm --filter @grasdouble/lufa_design-system-storybook run dev",
    "ds:storybook:lint": "pnpm --filter @grasdouble/lufa_design-system-storybook run lint",
    "ds:storybook:prettier": "pnpm --filter @grasdouble/lufa_design-system-storybook run prettier",
    "ds:tokens-----------------------------------------": "",
    "ds:tokens:build": "pnpm --filter @grasdouble/lufa_design-system-tokens run build",
    "ds:tokens:lint": "pnpm --filter @grasdouble/lufa_design-system-tokens run lint",
    "ds:tokens:prettier": "pnpm --filter @grasdouble/lufa_design-system-tokens run prettier",
    "plugin:vite---------------------------------------": "",
    "plugin:vite:lint": "pnpm --filter @grasdouble/lufa_plugin_vite_* run lint",
    "plugin:vite:prettier": "pnpm --filter @grasdouble/lufa_plugin_vite_* run prettier",
    "tools---------------------------------------------": "",
    "tools:generate-outdated-report": "node .github/actions/dependency-update/generateOutdatedReport.js"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@grasdouble/lufa_config_prettier": "workspace:^",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "concurrently": "^9.2.1",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20"
  },
  "packageManager": "pnpm@10.8.1"
}
