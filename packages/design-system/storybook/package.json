{
  "name": "@grasdouble/lufa_design-system-storybook",
  "version": "1.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "files": [
    "storybook-static",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "storybook build",
    "dev": "storybook dev -p 6006 --no-open",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "bash -c 'pnpm typecheck'"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "@grasdouble/lufa_design-system": "workspace:^",
    "@grasdouble/lufa_design-system-themes": "workspace:^",
    "@grasdouble/lufa_design-system-tokens": "workspace:^",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@grasdouble/lufa_config_eslint": "workspace:^",
    "@grasdouble/lufa_config_prettier": "workspace:^",
    "@grasdouble/lufa_config_tsconfig": "workspace:^",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/addon-docs": "^10.2.3",
    "@storybook/addon-themes": "^10.2.3",
    "@storybook/react-vite": "^10.2.3",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.2.3",
    "globals": "^17.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "storybook": "^10.2.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
