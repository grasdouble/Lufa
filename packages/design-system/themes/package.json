    "@types/node": "^24.10.4",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
