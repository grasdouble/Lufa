{
  "semantic": {
    "z-index": {
      "base": {
        "$value": "0",
        "$type": "number",
        "$description": "Base z-index for normal content flow",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "elevation",
            "useCase": "Default content, normal document flow",
            "themeable": true,
            "modeAware": false
          }
        }
      },
      "dropdown": {
        "$value": "1000",
        "$type": "number",
        "$description": "Z-index for dropdown menus - above normal content",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "elevation",
            "useCase": "Dropdown menus, select options, context menus",
            "themeable": true,
            "modeAware": false
          }
        }
      },
      "sticky": {
        "$value": "1100",
        "$type": "number",
        "$description": "Z-index for sticky positioned elements - above dropdowns",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "elevation",
            "useCase": "Sticky headers, sticky navigation, fixed toolbars",
            "themeable": true,
            "modeAware": false
          }
        }
      },
      "fixed": {
        "$value": "1200",
        "$type": "number",
        "$description": "Z-index for fixed positioned elements - above sticky elements",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "elevation",
            "useCase": "Fixed navigation bars, floating action buttons",
            "themeable": true,
            "modeAware": false
          }
        }
      },
      "modal-backdrop": {
        "$value": "1300",
        "$type": "number",
        "$description": "Z-index for modal backdrops - overlays page content",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "elevation",
            "useCase": "Modal overlays, dialog backdrops, drawer overlays",
            "themeable": true,
            "modeAware": false
          }
        }
      },
      "modal": {
        "$value": "1400",
        "$type": "number",
        "$description": "Z-index for modal content - above modal backdrop",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "elevation",
            "useCase": "Modal dialogs, drawer panels, lightboxes",
            "themeable": true,
            "modeAware": false
          }
        }
      },
      "popover": {
        "$value": "1500",
        "$type": "number",
        "$description": "Z-index for popovers and tooltips - above modals",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "elevation",
            "useCase": "Popovers, tooltips, floating hints",
            "themeable": true,
            "modeAware": false
          }
        }
      },
      "toast": {
        "$value": "1600",
        "$type": "number",
        "$description": "Z-index for toast notifications - highest layer for critical messages",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "elevation",
            "useCase": "Toast notifications, snackbars, system alerts",
            "themeable": true,
            "modeAware": false
          }
        }
      }
    }
  }
}
