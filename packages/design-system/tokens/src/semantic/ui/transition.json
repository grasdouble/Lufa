{
  "semantic": {
    "ui": {
      "transition": {
        "duration": {
          "fast": {
            "$value": "{primitive.motion.duration.fast}",
            "$type": "duration",
            "$description": "Fast transition duration for quick interactions (150ms)",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "transition",
                "useCase": "Button hovers, dropdown open, tooltip appear, quick feedback",
                "themeable": true,
                "modeAware": false
              }
            }
          },
          "normal": {
            "$value": "{primitive.motion.duration.normal}",
            "$type": "duration",
            "$description": "Normal transition duration for standard animations (250ms)",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "transition",
                "useCase": "Modal animations, panel slides, standard component transitions",
                "themeable": true,
                "modeAware": false
              }
            }
          },
          "slow": {
            "$value": "{primitive.motion.duration.slow}",
            "$type": "duration",
            "$description": "Slow transition duration for deliberate animations (400ms)",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "transition",
                "useCase": "Page transitions, complex animations, emphasized movements",
                "themeable": true,
                "modeAware": false
              }
            }
          }
        },
        "timing-function": {
          "default": {
            "$value": "{primitive.motion.easing.ease-in-out}",
            "$type": "cubicBezier",
            "$description": "Default timing function for smooth animations (ease-in-out)",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "transition",
                "useCase": "General purpose transitions, modal animations, smooth movements",
                "themeable": true,
                "modeAware": false
              }
            }
          }
        }
      }
    }
  }
}
