        "$value": "{core.semantic.error-border}",
        "$type": "color",
        "$description": "Border color for error contexts",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "ui",
            "useCase": "Error alert borders, invalid input borders, destructive action borders"
          }
