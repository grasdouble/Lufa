        "info": {
          "$value": "{core.semantic.info.default}",
          "$type": "color",
          "$description": "Text color for informational messages",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Info message text, help content, neutral notifications",
              "themeable": true,
              "modeAware": false
            }
          }
