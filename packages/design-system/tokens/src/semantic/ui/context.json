        "$description": "Text and icons on info background - for use with background-info (subtle background)",
        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "ui",
            "subcategory": "contrast",
            "useCase": "Text in info alerts, info badge text, info message content",
