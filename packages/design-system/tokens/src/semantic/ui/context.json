        "error": {
          "$value": "{core.semantic.error.default}",
          "$type": "color",
          "$description": "Text color for error messages and validation failures",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Error message text, validation errors, destructive action warnings",
              "themeable": true,
              "modeAware": false
            }
          }
        },
        "warning": {
          "$value": "{core.semantic.warning.default}",
          "$type": "color",
          "$description": "Text color for warning messages and caution notices",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Warning message text, caution labels, pending action notices",
