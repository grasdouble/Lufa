        "$extensions": {
          "lufa": {
            "level": "semantic",
            "category": "ui",
            "useCase": "Success message text, positive status indicators, confirmation text"
          }
        }
      },
      "text-error": {
        "$value": "{core.semantic.error}",
        "$type": "color",
