{
  "semantic": {
    "ui": {
      "background": {
        "page": {
          "$value": "{core.neutral.background}",
          "$type": "color",
          "$description": "Main page background - base canvas for entire application",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Root background, page wrapper, main container",
              "themeable": true,
              "modeAware": false
            }
          }
        },
        "surface": {
          "$value": "{core.neutral.surface.default}",
          "$type": "color",
          "$description": "Surface background for cards, panels, and elevated content",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Cards, panels, modal backgrounds, dropdown backgrounds",
              "themeable": true,
              "modeAware": false
            }
          }
        },
        "success": {
          "$value": "{core.semantic.success.subtle}",
          "$type": "color",
          "$description": "Background for success messages and positive feedback",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Success alerts, positive notifications, confirmation banners",
              "themeable": true,
              "modeAware": false
            }
          }
        },
        "error": {
          "$value": "{core.semantic.error.subtle}",
          "$type": "color",
          "$description": "Background for error messages and validation failures",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Error alerts, validation messages, destructive action confirmations",
              "themeable": true,
              "modeAware": false
            }
          }
        },
        "warning": {
          "$value": "{core.semantic.warning.subtle}",
          "$type": "color",
          "$description": "Background for warning messages and caution notices",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Warning alerts, caution banners, pending action notices",
              "themeable": true,
              "modeAware": false
            }
          }
        },
        "info": {
          "$value": "{core.semantic.info.subtle}",
          "$type": "color",
          "$description": "Background for informational messages and neutral notifications",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "useCase": "Info alerts, tips, help messages, neutral notifications",
              "themeable": true,
              "modeAware": false
            }
          }
        },
        "on": {
          "success": {
            "$value": "{core.semantic.success.default}",
            "$type": "color",
            "$description": "Text and icons on success background - for use with background-success (subtle background)",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "contrast",
                "useCase": "Text in success alerts, success badge text, success message content",
                "pairedWith": "background-success",
                "themeable": true,
                "modeAware": false
              }
            }
          },
          "error": {
            "$value": "{core.semantic.error.default}",
            "$type": "color",
            "$description": "Text and icons on error background - for use with background-error (subtle background)",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "contrast",
                "useCase": "Text in error alerts, error badge text, error message content",
                "pairedWith": "background-error",
                "themeable": true,
                "modeAware": false
              }
            }
          },
          "warning": {
            "$value": "{core.semantic.warning.default}",
            "$type": "color",
            "$description": "Text and icons on warning background - for use with background-warning (subtle background)",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "contrast",
                "useCase": "Text in warning alerts, warning badge text, warning message content",
                "pairedWith": "background-warning",
                "themeable": true,
                "modeAware": false
              }
            }
          },
          "info": {
            "$value": "{core.semantic.info.default}",
            "$type": "color",
            "$description": "Text and icons on info background - for use with background-info (subtle background)",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "contrast",
                "useCase": "Text in info alerts, info badge text, info message content",
                "pairedWith": "background-info",
                "themeable": true,
                "modeAware": false
              }
            }
          },
          "primary": {
            "$value": "{primitive.color.hc.white}",
            "$type": "color",
            "$description": "Text and icons on primary background - AAA contrast guaranteed with primary brand color",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "contrast",
                "useCase": "Text/icons on primary buttons, primary badges, primary interactive elements",
                "pairedWith": "core.brand.primary",
                "modes": {
                  "light": "{primitive.color.hc.white}",
                  "dark": "{primitive.color.hc.white}",
                  "high-contrast": "{primitive.color.hc.white}"
                },
                "a11y": {
                  "contrastRatio": {
                    "onBlue600": 7.5
                  },
                  "wcagLevel": "AAA"
                },
                "themeable": true,
                "modeAware": true
              }
            }
          },
          "secondary": {
            "$value": "{primitive.color.hc.white}",
            "$type": "color",
            "$description": "Text and icons on secondary background - AAA contrast guaranteed with secondary brand color",
            "$extensions": {
              "lufa": {
                "level": "semantic",
                "category": "ui",
                "subcategory": "contrast",
                "useCase": "Text/icons on secondary buttons, secondary badges, secondary interactive elements",
                "pairedWith": "core.brand.secondary",
                "modes": {
                  "light": "{primitive.color.hc.white}",
                  "dark": "{primitive.color.hc.white}",
                  "high-contrast": "{primitive.color.hc.white}"
                },
                "a11y": {
                  "contrastRatio": {
                    "onPurple500": 7.2
                  },
                  "wcagLevel": "AAA"
                },
                "themeable": true,
                "modeAware": true
              }
            }
          }
        }
      },
      "overlay": {
        "backdrop": {
          "$value": "{primitive.color.alpha.black.50}",
          "$type": "color",
          "$description": "Semi-transparent backdrop for modals, dialogs, and drawers",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "subcategory": "overlay",
              "useCase": "Modal backdrops, dialog overlays, drawer backgrounds",
              "modes": {
                "light": "{primitive.color.alpha.black.50}",
                "dark": "{primitive.color.alpha.black.80}",
                "high-contrast": "{primitive.color.alpha.black.90}"
              },
              "themeable": true,
              "modeAware": true
            }
          }
        },
        "hover": {
          "$value": "{primitive.color.alpha.black.4}",
          "$type": "color",
          "$description": "Subtle overlay for hover states on interactive elements",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "subcategory": "overlay",
              "useCase": "Hover state backgrounds for list items, cards, table rows",
              "modes": {
                "light": "{primitive.color.alpha.black.4}",
                "dark": "{primitive.color.alpha.white.8}",
                "high-contrast": "{primitive.color.alpha.white.16}"
              },
              "themeable": true,
              "modeAware": true
            }
          }
        },
        "pressed": {
          "$value": "{primitive.color.alpha.black.8}",
          "$type": "color",
          "$description": "Overlay for pressed/active states",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "subcategory": "overlay",
              "useCase": "Pressed/active state overlays on buttons and interactive elements",
              "modes": {
                "light": "{primitive.color.alpha.black.8}",
                "dark": "{primitive.color.alpha.white.16}",
                "high-contrast": "{primitive.color.alpha.white.16}"
              },
              "themeable": true,
              "modeAware": true
            }
          }
        },
        "selected": {
          "$value": "{primitive.color.alpha.black.16}",
          "$type": "color",
          "$description": "Overlay for selected states in lists and menus",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "subcategory": "overlay",
              "useCase": "Selected list items, active menu items, selected tabs",
              "modes": {
                "light": "{primitive.color.alpha.black.16}",
                "dark": "{primitive.color.alpha.white.16}",
                "high-contrast": "{primitive.color.alpha.white.16}"
              },
              "themeable": true,
              "modeAware": true
            }
          }
        },
        "scrim": {
          "$value": "{primitive.color.alpha.black.38}",
          "$type": "color",
          "$description": "Scrim overlay for bottom sheets and temporary surfaces",
          "$extensions": {
            "lufa": {
              "level": "semantic",
              "category": "ui",
              "subcategory": "overlay",
              "useCase": "Bottom sheets, temporary drawers, transient overlays",
              "modes": {
                "light": "{primitive.color.alpha.black.38}",
                "dark": "{primitive.color.alpha.black.60}",
                "high-contrast": "{primitive.color.alpha.black.80}"
              },
              "themeable": true,
              "modeAware": true
            }
          }
        }
      },
      "text": {
