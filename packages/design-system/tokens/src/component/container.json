          "$extensions": {
            "lufa": {
              "level": "component",
              "category": "layout",
              "useCase": "container max-width"
            }
          }
        },
        "md": {
          "$value": "{semantic.layout.breakpoint.md}",
          "$type": "dimension",
          "$description": "Max-width for medium screens",
          "$extensions": {
            "lufa": {
              "level": "component",
              "category": "layout",
              "useCase": "container max-width"
            }
          }
        },
        "lg": {
          "$value": "{semantic.layout.breakpoint.lg}",
          "$type": "dimension",
