{
  "primitive": {
    "motion": {
      "duration": {
        "instant": {
          "$value": "100ms",
          "$type": "duration",
          "$description": "Instant duration for immediate feedback (100ms)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Hover feedback, toggle switches, immediate interactions",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "fast": {
          "$value": "150ms",
          "$type": "duration",
          "$description": "Fast duration for quick transitions (150ms)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Button clicks, dropdown open/close, tooltip appear",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "normal": {
          "$value": "300ms",
          "$type": "duration",
          "$description": "Normal duration for standard transitions (300ms)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Modal open, panel slide, standard animations",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "slow": {
          "$value": "500ms",
          "$type": "duration",
          "$description": "Slow duration for deliberate transitions (500ms)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Page transitions, complex animations, emphasized movement",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "slower": {
          "$value": "2s",
          "$type": "duration",
          "$description": "Very slow duration for continuous animations (2s)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Pulse animations, breathing effects, shimmer loading states",
              "themeable": false,
              "modeAware": false
            }
          }
        }
      },
      "easing": {
        "linear": {
          "$value": "cubic-bezier(0, 0, 1, 1)",
          "$type": "cubicBezier",
          "$description": "Linear easing - constant speed (no acceleration)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Progress bars, loading indicators, mechanical movement",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease-in": {
          "$value": "cubic-bezier(0.4, 0, 1, 1)",
          "$type": "cubicBezier",
          "$description": "Ease-in - slow start, fast end (accelerating)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Elements leaving screen, fade out, collapse",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease-out": {
          "$value": "cubic-bezier(0, 0, 0.2, 1)",
          "$type": "cubicBezier",
          "$description": "Ease-out - fast start, slow end (decelerating)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Elements entering screen, fade in, expand",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease-in-out": {
          "$value": "cubic-bezier(0.4, 0, 0.2, 1)",
          "$type": "cubicBezier",
          "$description": "Ease-in-out - slow start and end (smooth)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Modal open/close, smooth transitions, general purpose",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease": {
          "$value": "ease",
          "$type": "cubicBezier",
          "$description": "Standard ease - browser default easing",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "General purpose transitions, light mode defaults",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease-bounce": {
          "$value": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
          "$type": "cubicBezier",
          "$description": "Bounce easing - overshoots and settles back (playful)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Playful interactions, energetic themes (volcano, cyberpunk)",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease-elastic": {
          "$value": "cubic-bezier(0.175, 0.885, 0.32, 1.275)",
          "$type": "cubicBezier",
          "$description": "Elastic easing - slight overshoot (energetic)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Energetic themes, dynamic interactions (volt)",
              "themeable": false,
              "modeAware": false
            }
          }
        }
      }
    }
  }
}
