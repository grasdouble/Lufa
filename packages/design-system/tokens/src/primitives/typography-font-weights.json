{
  "primitive": {
    "typography": {
      "font-weight": {
        "normal": {
          "$value": "400",
          "$type": "fontWeight",
          "$description": "Normal font weight - used for standard body text",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "typography",
              "useCase": "Body text, paragraphs, descriptions",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "medium": {
          "$value": "500",
          "$type": "fontWeight",
          "$description": "Medium font weight - used for subtle emphasis",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "typography",
              "useCase": "Labels, navigation, light emphasis",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "semibold": {
          "$value": "600",
          "$type": "fontWeight",
          "$description": "Semibold font weight - used for subtitles and strong emphasis",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "typography",
              "useCase": "Subtitles, buttons, strong emphasis",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "bold": {
          "$value": "700",
          "$type": "fontWeight",
          "$description": "Bold font weight - used for headings and important elements",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "typography",
              "useCase": "Headings, critical interface elements",
              "themeable": false,
              "modeAware": false
            }
          }
        }
      }
    }
  }
}
