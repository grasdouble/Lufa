{
  "primitive": {
    "motion": {
      "duration": {
        "instant": {
          "$value": "100ms",
          "$type": "duration",
          "$description": "Instant duration for immediate feedback (100ms)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Hover feedback, toggle switches, immediate interactions",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "fast": {
          "$value": "150ms",
          "$type": "duration",
          "$description": "Fast duration for quick transitions (150ms)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Button clicks, dropdown open/close, tooltip appear",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "normal": {
          "$value": "250ms",
          "$type": "duration",
          "$description": "Normal duration for standard transitions (250ms)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Modal open, panel slide, standard animations",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "slow": {
          "$value": "400ms",
          "$type": "duration",
          "$description": "Slow duration for deliberate transitions (400ms)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Page transitions, complex animations, emphasized movement",
              "themeable": false,
              "modeAware": false
            }
          }
        }
      },
      "easing": {
        "linear": {
          "$value": "cubic-bezier(0, 0, 1, 1)",
          "$type": "cubicBezier",
          "$description": "Linear easing - constant speed (no acceleration)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Progress bars, loading indicators, mechanical movement",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease-in": {
          "$value": "cubic-bezier(0.4, 0, 1, 1)",
          "$type": "cubicBezier",
          "$description": "Ease-in - slow start, fast end (accelerating)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Elements leaving screen, fade out, collapse",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease-out": {
          "$value": "cubic-bezier(0, 0, 0.2, 1)",
          "$type": "cubicBezier",
          "$description": "Ease-out - fast start, slow end (decelerating)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Elements entering screen, fade in, expand",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "ease-in-out": {
          "$value": "cubic-bezier(0.4, 0, 0.2, 1)",
          "$type": "cubicBezier",
          "$description": "Ease-in-out - slow start and end (smooth)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "motion",
              "useCase": "Modal open/close, smooth transitions, general purpose",
              "themeable": false,
              "modeAware": false
            }
          }
        }
      }
    }
  }
}
