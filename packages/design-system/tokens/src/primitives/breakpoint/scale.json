{
  "primitive": {
    "breakpoint": {
      "xs": {
        "$value": "320px",
        "$type": "dimension",
        "$description": "Extra small - Mobile portrait (320px+)",
        "$extensions": {
          "lufa": {
            "level": "primitive",
            "category": "breakpoint",
            "useCase": "mobile-first baseline",
            "devices": "small phones (320-639px)",
            "mediaQuery": "@media (min-width: 320px)"
          }
        }
      },
      "sm": {
        "$value": "640px",
        "$type": "dimension",
        "$description": "Small - Mobile landscape, large phones (640px+)",
        "$extensions": {
          "lufa": {
            "level": "primitive",
            "category": "breakpoint",
            "useCase": "small tablet, large mobile",
            "devices": "large phones (640-767px)",
            "mediaQuery": "@media (min-width: 640px)"
          }
        }
      },
      "md": {
        "$value": "768px",
        "$type": "dimension",
        "$description": "Medium - Tablets portrait (768px+)",
        "$extensions": {
          "lufa": {
            "level": "primitive",
            "category": "breakpoint",
            "useCase": "tablet layouts",
            "devices": "tablets portrait (768-1023px)",
            "mediaQuery": "@media (min-width: 768px)"
          }
        }
      },
      "lg": {
        "$value": "1024px",
        "$type": "dimension",
        "$description": "Large - Tablets landscape, small desktops (1024px+)",
        "$extensions": {
          "lufa": {
            "level": "primitive",
            "category": "breakpoint",
            "useCase": "desktop layouts",
            "devices": "tablets landscape, small desktops (1024-1279px)",
            "mediaQuery": "@media (min-width: 1024px)"
          }
        }
      },
      "xl": {
        "$value": "1280px",
        "$type": "dimension",
        "$description": "Extra large - Desktop (1280px+)",
        "$extensions": {
          "lufa": {
            "level": "primitive",
            "category": "breakpoint",
            "useCase": "standard desktop",
            "devices": "desktops (1280-1535px)",
            "mediaQuery": "@media (min-width: 1280px)"
          }
        }
      },
      "2xl": {
        "$value": "1536px",
        "$type": "dimension",
        "$description": "2X Extra large - Large desktop, ultra-wide (1536px+)",
        "$extensions": {
          "lufa": {
            "level": "primitive",
            "category": "breakpoint",
            "useCase": "large screens",
            "devices": "large desktops (1536px+)",
            "mediaQuery": "@media (min-width: 1536px)"
          }
        }
      }
    }
  }
}
