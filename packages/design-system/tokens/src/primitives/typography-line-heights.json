{
  "primitive": {
    "typography": {
      "line-height": {
        "tight": {
          "$value": "1.25",
          "$type": "number",
          "$description": "Tight line height - used for headings and short text",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "typography",
              "useCase": "Headings, compact interface text",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "normal": {
          "$value": "1.5",
          "$type": "number",
          "$description": "Normal line height - used for standard body text (WCAG recommended)",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "typography",
              "useCase": "Body text, paragraphs (optimal accessibility)",
              "themeable": false,
              "modeAware": false
            }
          }
        },
        "relaxed": {
          "$value": "1.75",
          "$type": "number",
          "$description": "Relaxed line height - used for better readability of long text",
          "$extensions": {
            "lufa": {
              "level": "primitive",
              "category": "typography",
              "useCase": "Long articles, documentation, comfortable reading",
              "themeable": false,
              "modeAware": false
            }
          }
        }
      }
    }
  }
}
