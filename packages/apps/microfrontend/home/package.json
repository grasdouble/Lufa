{
  "name": "@grasdouble/lufa_microfrontend_home",
  "version": "0.3.4",
  "private": false,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/home.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build -w",
    "dev": "vite",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@grasdouble/lufa_design-system": "workspace:^",
    "clsx": "^2.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@grasdouble/lufa_config_eslint": "workspace:^",
    "@grasdouble/lufa_config_prettier": "workspace:^",
    "@grasdouble/lufa_config_tsconfig": "workspace:^",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-externalize-deps": "^0.10.0",
    "vite-plugin-static-copy": "^3.2.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
