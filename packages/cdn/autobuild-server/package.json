{
  "name": "@grasdouble/cdn_autobuild-server",
  "version": "0.0.1",
  "description": "Self-fed CDN from npm packages",
  "main": "dist/index.mjs",
  "type": "module",
  "scripts": {
    "start": "node dist/index.mjs",
    "dev": "nodemon src/index.js",
    "build": "esbuild src/index.js --bundle --outfile=dist/index.mjs --platform=node --format=esm",
    "build:cjs": "esbuild src/index.js --bundle --outfile=dist/index.cjs --platform=node"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "esbuild": "^0.25.2",
    "express": "^5.1.0",
    "fs-extra": "^11.2.0",
    "pacote": "^21.0.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  }
}
